//compile_error!("This is just a template page and should not be compiled");


#[allow(nonstandard_style, dead_code, unreachable_patterns)]
mod parser {
	//["Fact", "Start", "Sum", "Term"]
	//["$", "id", "int", "lparen", "mult", "plus", "rparen"]

	use parse::lex::{self, Tokens};
	use parse::{
		SyntaxError,
		Error,
		Parser as ParserTrait,
		ast::{self, AstNode, reverse},
	};

	use super::lexer::TokenKind;

	pub type Token<'a> = lex::Token<'a, TokenKind>;

	#[derive(Debug, Clone, Copy, PartialEq, Eq, PartialOrd, Ord, Hash)]
	pub enum NonTerm {
		N_Fact,
		N_Start,
		N_Sum,
		N_Term,
	}

	mod display {
		use std::fmt::{Display, Formatter, Result};
		use super::NonTerm;

		impl Display for NonTerm {
			fn fmt(&self, f: &mut Formatter) -> Result {
				let name = match self {
					NonTerm::N_Fact => "Fact",
					NonTerm::N_Start => "Start",
					NonTerm::N_Sum => "Sum",
					NonTerm::N_Term => "Term",
				};
				write!(f, "{}", name)
			}
		}
	}

	const START: NonTerm = NonTerm::N_Start;

	pub type Ast<'a> = ast::Ast<NonTerm, Token<'a>>;

	type Result<T> = std::result::Result<T, Error>;

	type Res<'a> = Result<(Ast<'a>, usize)>;

	fn ret<'a>(label: NonTerm, children: AstNode<NonTerm, Token<'a>>, n: usize) -> Res<'a> {
		Ok((Ast::new(label, vec![children]), n))
	}

	fn goto_err<'a>() -> Res<'a> {
		panic!("Unexpected error in parsing")
	}

	pub struct Parser<'a, L> {
		inp: L,
		la: Option<Token<'a>>,
	}

	impl<'a, L> Parser<'a, L> {
		fn la(&self) -> &Option<Token<'a>> {
			&self.la
		}

		fn kind(&self) -> Option<TokenKind> {
			match self.la() {
				Some(tok) => Some(*tok.kind()),
				None => None,
			}
		}
	}

	impl<'a, L> Parser<'a, L>
	where
		L: Tokens<'a, TokenKind>,
	{

		fn err_token(&self, tok: &Token<'a>) -> SyntaxError {
			let data = tok.get_error_data();
			SyntaxError::unexpected_token(tok.val(), data)
		}

		fn err_eof(&self) -> SyntaxError {
			let data = self.inp.current_err_data();
			SyntaxError::unexpected_eof(data)
		}

		fn err(&self) -> SyntaxError {
			match self.la() {
				Some(tok) => self.err_token(tok),
				None => self.err_eof(),
			}
		}

		fn shift(&mut self) -> Result<Option<Token<'a>>> {
			let la = self.la;
			self.la = self.inp.next().transpose()?;
			Ok(la)
		}

		fn s0(&mut self) -> Result<Ast<'a>> {
			//0.
			//kernel:
			//	[Start -> ·Sum, $]*
			//closure:
			//	[Fact -> ·Fact mult Term, plus]
			//	[Term -> ·id, plus]
			//	[Fact -> ·Term, $]
			//	[Fact -> ·Fact mult Term, mult]
			//	[Fact -> ·Term, mult]
			//	[Fact -> ·Fact mult Term, $]
			//	[Term -> ·id, mult]
			//	[Sum -> ·Sum plus Fact, $]
			//	[Sum -> ·Fact, plus]
			//	[Sum -> ·Sum plus Fact, plus]
			//	[Term -> ·lparen Sum rparen, plus]
			//	[Term -> ·lparen Sum rparen, $]
			//	[Term -> ·int, plus]
			//	[Fact -> ·Term, plus]
			//	[Term -> ·int, mult]
			//	[Term -> ·lparen Sum rparen, mult]
			//	[Term -> ·id, $]
			//	[Term -> ·int, $]
			//	[Sum -> ·Fact, $]
			//transitions:
			//	lparen -> 4
			//	Sum -> 2
			//	int -> 5
			//	Term -> 3
			//	id -> 6
			//	Fact -> 1
			let mut res: (Ast<'a>, usize) = match self.kind() {
				Some(TokenKind::T_lparen) => { let la = self.shift()?.unwrap(); self.s4(AstNode::Leaf(la)) }
				Some(TokenKind::T_int) => { let la = self.shift()?.unwrap(); self.s5(AstNode::Leaf(la)) }
				Some(TokenKind::T_id) => { let la = self.shift()?.unwrap(); self.s6(AstNode::Leaf(la)) }
				_ => Err(Error::from(self.err())),
			}?;
			loop {
				let (s, n) = res;
				if n == 0 {
					res = match *s.label() {
						NonTerm::N_Sum => self.s2(AstNode::Branch(s)),
						NonTerm::N_Term => self.s3(AstNode::Branch(s)),
						NonTerm::N_Fact => self.s1(AstNode::Branch(s)),
						START => return Ok(s),
						_ => goto_err(),
					}?;
				} else {
					panic!("Unexpected error in parsing")
				};
			};
		}

		fn s1(&mut self, val: AstNode<NonTerm, Token<'a>>) -> Res<'a> {
			//1.
			//kernel:
			//	[Fact -> Fact ·mult Term, $]*
			//	[Sum -> Fact·, $]*
			//	[Fact -> Fact ·mult Term, plus]*
			//	[Sum -> Fact·, plus]*
			//	[Fact -> Fact ·mult Term, mult]*
			//closure:
			//transitions:
			//	mult -> 7
			let mut res: (Ast<'a>, usize) = match self.kind() {
				Some(TokenKind::T_mult) => { let la = self.shift()?.unwrap(); self.s7(AstNode::Leaf(la)) }
				None => return ret(NonTerm::N_Sum, val, 0),
				Some(TokenKind::T_plus) => return ret(NonTerm::N_Sum, val, 0),
				_ => Err(Error::from(self.err())),
			}?;
			loop {
				let (mut s, n) = res;
				if n == 0 {
					res = match *s.label() {
						_ => goto_err(),
					}?;
				} else {
					s.push(val);
					return Ok((s, n-1));
				};
			};
		}

		fn s2(&mut self, val: AstNode<NonTerm, Token<'a>>) -> Res<'a> {
			//2.
			//kernel:
			//	[Start -> Sum·, $]*
			//	[Sum -> Sum ·plus Fact, $]*
			//	[Sum -> Sum ·plus Fact, plus]*
			//closure:
			//transitions:
			//	plus -> 8
			let mut res: (Ast<'a>, usize) = match self.kind() {
				Some(TokenKind::T_plus) => { let la = self.shift()?.unwrap(); self.s8(AstNode::Leaf(la)) }
				None => return ret(NonTerm::N_Start, val, 0),
				_ => Err(Error::from(self.err())),
			}?;
			loop {
				let (mut s, n) = res;
				if n == 0 {
					res = match *s.label() {
						_ => goto_err(),
					}?;
				} else {
					s.push(val);
					return Ok((s, n-1));
				};
			};
		}

		fn s3(&mut self, val: AstNode<NonTerm, Token<'a>>) -> Res<'a> {
			//3.
			//kernel:
			//	[Fact -> Term·, mult]*
			//	[Fact -> Term·, plus]*
			//	[Fact -> Term·, $]*
			//	[Fact -> Term·, rparen]*
			//closure:
			//transitions:
			let mut res: (Ast<'a>, usize) = match self.kind() {
				Some(TokenKind::T_mult) => return ret(NonTerm::N_Fact, val, 0),
				Some(TokenKind::T_plus) => return ret(NonTerm::N_Fact, val, 0),
				None => return ret(NonTerm::N_Fact, val, 0),
				Some(TokenKind::T_rparen) => return ret(NonTerm::N_Fact, val, 0),
				_ => Err(Error::from(self.err())),
			}?;
			loop {
				let (mut s, n) = res;
				if n == 0 {
					res = match *s.label() {
						_ => goto_err(),
					}?;
				} else {
					s.push(val);
					return Ok((s, n-1));
				};
			};
		}

		fn s4(&mut self, val: AstNode<NonTerm, Token<'a>>) -> Res<'a> {
			//4.
			//kernel:
			//	[Term -> lparen ·Sum rparen, rparen]*
			//	[Term -> lparen ·Sum rparen, plus]*
			//	[Term -> lparen ·Sum rparen, $]*
			//	[Term -> lparen ·Sum rparen, mult]*
			//closure:
			//	[Fact -> ·Term, rparen]
			//	[Fact -> ·Fact mult Term, plus]
			//	[Term -> ·id, plus]
			//	[Fact -> ·Fact mult Term, rparen]
			//	[Sum -> ·Sum plus Fact, plus]
			//	[Term -> ·lparen Sum rparen, plus]
			//	[Sum -> ·Fact, plus]
			//	[Sum -> ·Sum plus Fact, rparen]
			//	[Term -> ·int, mult]
			//	[Term -> ·int, rparen]
			//	[Fact -> ·Term, plus]
			//	[Fact -> ·Term, mult]
			//	[Term -> ·id, rparen]
			//	[Term -> ·id, mult]
			//	[Term -> ·int, plus]
			//	[Term -> ·lparen Sum rparen, mult]
			//	[Sum -> ·Fact, rparen]
			//	[Term -> ·lparen Sum rparen, rparen]
			//	[Fact -> ·Fact mult Term, mult]
			//transitions:
			//	id -> 11
			//	lparen -> 4
			//	Fact -> 10
			//	int -> 13
			//	Term -> 9
			//	Sum -> 12
			let mut res: (Ast<'a>, usize) = match self.kind() {
				Some(TokenKind::T_id) => { let la = self.shift()?.unwrap(); self.s11(AstNode::Leaf(la)) }
				Some(TokenKind::T_lparen) => { let la = self.shift()?.unwrap(); self.s4(AstNode::Leaf(la)) }
				Some(TokenKind::T_int) => { let la = self.shift()?.unwrap(); self.s13(AstNode::Leaf(la)) }
				_ => Err(Error::from(self.err())),
			}?;
			loop {
				let (mut s, n) = res;
				if n == 0 {
					res = match *s.label() {
						NonTerm::N_Fact => self.s10(AstNode::Branch(s)),
						NonTerm::N_Term => self.s9(AstNode::Branch(s)),
						NonTerm::N_Sum => self.s12(AstNode::Branch(s)),
						_ => goto_err(),
					}?;
				} else {
					s.push(val);
					return Ok((s, n-1));
				};
			};
		}

		fn s5(&mut self, val: AstNode<NonTerm, Token<'a>>) -> Res<'a> {
			//5.
			//kernel:
			//	[Term -> int·, plus]*
			//	[Term -> int·, mult]*
			//	[Term -> int·, $]*
			//	[Term -> int·, rparen]*
			//closure:
			//transitions:
			let mut res: (Ast<'a>, usize) = match self.kind() {
				Some(TokenKind::T_plus) => return ret(NonTerm::N_Term, val, 0),
				Some(TokenKind::T_mult) => return ret(NonTerm::N_Term, val, 0),
				None => return ret(NonTerm::N_Term, val, 0),
				Some(TokenKind::T_rparen) => return ret(NonTerm::N_Term, val, 0),
				_ => Err(Error::from(self.err())),
			}?;
			loop {
				let (mut s, n) = res;
				if n == 0 {
					res = match *s.label() {
						_ => goto_err(),
					}?;
				} else {
					s.push(val);
					return Ok((s, n-1));
				};
			};
		}

		fn s6(&mut self, val: AstNode<NonTerm, Token<'a>>) -> Res<'a> {
			//6.
			//kernel:
			//	[Term -> id·, $]*
			//	[Term -> id·, mult]*
			//	[Term -> id·, rparen]*
			//	[Term -> id·, plus]*
			//closure:
			//transitions:
			let mut res: (Ast<'a>, usize) = match self.kind() {
				None => return ret(NonTerm::N_Term, val, 0),
				Some(TokenKind::T_mult) => return ret(NonTerm::N_Term, val, 0),
				Some(TokenKind::T_rparen) => return ret(NonTerm::N_Term, val, 0),
				Some(TokenKind::T_plus) => return ret(NonTerm::N_Term, val, 0),
				_ => Err(Error::from(self.err())),
			}?;
			loop {
				let (mut s, n) = res;
				if n == 0 {
					res = match *s.label() {
						_ => goto_err(),
					}?;
				} else {
					s.push(val);
					return Ok((s, n-1));
				};
			};
		}

		fn s7(&mut self, val: AstNode<NonTerm, Token<'a>>) -> Res<'a> {
			//7.
			//kernel:
			//	[Fact -> Fact mult ·Term, $]*
			//	[Fact -> Fact mult ·Term, rparen]*
			//	[Fact -> Fact mult ·Term, mult]*
			//	[Fact -> Fact mult ·Term, plus]*
			//closure:
			//	[Term -> ·id, rparen]
			//	[Term -> ·int, rparen]
			//	[Term -> ·lparen Sum rparen, rparen]
			//	[Term -> ·int, $]
			//	[Term -> ·id, mult]
			//	[Term -> ·lparen Sum rparen, plus]
			//	[Term -> ·id, plus]
			//	[Term -> ·id, $]
			//	[Term -> ·lparen Sum rparen, mult]
			//	[Term -> ·int, mult]
			//	[Term -> ·lparen Sum rparen, $]
			//	[Term -> ·int, plus]
			//transitions:
			//	int -> 5
			//	lparen -> 4
			//	id -> 6
			//	Term -> 14
			let mut res: (Ast<'a>, usize) = match self.kind() {
				Some(TokenKind::T_int) => { let la = self.shift()?.unwrap(); self.s5(AstNode::Leaf(la)) }
				Some(TokenKind::T_lparen) => { let la = self.shift()?.unwrap(); self.s4(AstNode::Leaf(la)) }
				Some(TokenKind::T_id) => { let la = self.shift()?.unwrap(); self.s6(AstNode::Leaf(la)) }
				_ => Err(Error::from(self.err())),
			}?;
			loop {
				let (mut s, n) = res;
				if n == 0 {
					res = match *s.label() {
						NonTerm::N_Term => self.s14(AstNode::Branch(s)),
						_ => goto_err(),
					}?;
				} else {
					s.push(val);
					return Ok((s, n-1));
				};
			};
		}

		fn s8(&mut self, val: AstNode<NonTerm, Token<'a>>) -> Res<'a> {
			//8.
			//kernel:
			//	[Sum -> Sum plus ·Fact, $]*
			//	[Sum -> Sum plus ·Fact, plus]*
			//	[Sum -> Sum plus ·Fact, rparen]*
			//closure:
			//	[Term -> ·int, $]
			//	[Term -> ·id, plus]
			//	[Term -> ·int, mult]
			//	[Fact -> ·Fact mult Term, rparen]
			//	[Term -> ·lparen Sum rparen, rparen]
			//	[Fact -> ·Term, mult]
			//	[Fact -> ·Term, $]
			//	[Term -> ·id, $]
			//	[Term -> ·lparen Sum rparen, mult]
			//	[Term -> ·id, rparen]
			//	[Term -> ·id, mult]
			//	[Fact -> ·Fact mult Term, plus]
			//	[Fact -> ·Fact mult Term, mult]
			//	[Fact -> ·Fact mult Term, $]
			//	[Term -> ·int, rparen]
			//	[Fact -> ·Term, rparen]
			//	[Term -> ·lparen Sum rparen, $]
			//	[Term -> ·int, plus]
			//	[Fact -> ·Term, plus]
			//	[Term -> ·lparen Sum rparen, plus]
			//transitions:
			//	int -> 5
			//	Fact -> 15
			//	lparen -> 4
			//	id -> 6
			//	Term -> 3
			let mut res: (Ast<'a>, usize) = match self.kind() {
				Some(TokenKind::T_int) => { let la = self.shift()?.unwrap(); self.s5(AstNode::Leaf(la)) }
				Some(TokenKind::T_lparen) => { let la = self.shift()?.unwrap(); self.s4(AstNode::Leaf(la)) }
				Some(TokenKind::T_id) => { let la = self.shift()?.unwrap(); self.s6(AstNode::Leaf(la)) }
				_ => Err(Error::from(self.err())),
			}?;
			loop {
				let (mut s, n) = res;
				if n == 0 {
					res = match *s.label() {
						NonTerm::N_Fact => self.s15(AstNode::Branch(s)),
						NonTerm::N_Term => self.s3(AstNode::Branch(s)),
						_ => goto_err(),
					}?;
				} else {
					s.push(val);
					return Ok((s, n-1));
				};
			};
		}

		fn s9(&mut self, val: AstNode<NonTerm, Token<'a>>) -> Res<'a> {
			//9.
			//kernel:
			//	[Fact -> Term·, plus]*
			//	[Fact -> Term·, mult]*
			//	[Fact -> Term·, rparen]*
			//closure:
			//transitions:
			let mut res: (Ast<'a>, usize) = match self.kind() {
				Some(TokenKind::T_plus) => return ret(NonTerm::N_Fact, val, 0),
				Some(TokenKind::T_mult) => return ret(NonTerm::N_Fact, val, 0),
				Some(TokenKind::T_rparen) => return ret(NonTerm::N_Fact, val, 0),
				_ => Err(Error::from(self.err())),
			}?;
			loop {
				let (mut s, n) = res;
				if n == 0 {
					res = match *s.label() {
						_ => goto_err(),
					}?;
				} else {
					s.push(val);
					return Ok((s, n-1));
				};
			};
		}

		fn s10(&mut self, val: AstNode<NonTerm, Token<'a>>) -> Res<'a> {
			//10.
			//kernel:
			//	[Fact -> Fact ·mult Term, rparen]*
			//	[Sum -> Fact·, rparen]*
			//	[Sum -> Fact·, plus]*
			//	[Fact -> Fact ·mult Term, plus]*
			//	[Fact -> Fact ·mult Term, mult]*
			//closure:
			//transitions:
			//	mult -> 7
			let mut res: (Ast<'a>, usize) = match self.kind() {
				Some(TokenKind::T_mult) => { let la = self.shift()?.unwrap(); self.s7(AstNode::Leaf(la)) }
				Some(TokenKind::T_rparen) => return ret(NonTerm::N_Sum, val, 0),
				Some(TokenKind::T_plus) => return ret(NonTerm::N_Sum, val, 0),
				_ => Err(Error::from(self.err())),
			}?;
			loop {
				let (mut s, n) = res;
				if n == 0 {
					res = match *s.label() {
						_ => goto_err(),
					}?;
				} else {
					s.push(val);
					return Ok((s, n-1));
				};
			};
		}

		fn s11(&mut self, val: AstNode<NonTerm, Token<'a>>) -> Res<'a> {
			//11.
			//kernel:
			//	[Term -> id·, plus]*
			//	[Term -> id·, rparen]*
			//	[Term -> id·, mult]*
			//closure:
			//transitions:
			let mut res: (Ast<'a>, usize) = match self.kind() {
				Some(TokenKind::T_plus) => return ret(NonTerm::N_Term, val, 0),
				Some(TokenKind::T_rparen) => return ret(NonTerm::N_Term, val, 0),
				Some(TokenKind::T_mult) => return ret(NonTerm::N_Term, val, 0),
				_ => Err(Error::from(self.err())),
			}?;
			loop {
				let (mut s, n) = res;
				if n == 0 {
					res = match *s.label() {
						_ => goto_err(),
					}?;
				} else {
					s.push(val);
					return Ok((s, n-1));
				};
			};
		}

		fn s12(&mut self, val: AstNode<NonTerm, Token<'a>>) -> Res<'a> {
			//12.
			//kernel:
			//	[Term -> lparen Sum ·rparen, plus]*
			//	[Term -> lparen Sum ·rparen, mult]*
			//	[Sum -> Sum ·plus Fact, plus]*
			//	[Term -> lparen Sum ·rparen, rparen]*
			//	[Sum -> Sum ·plus Fact, rparen]*
			//	[Term -> lparen Sum ·rparen, $]*
			//closure:
			//transitions:
			//	rparen -> 16
			//	plus -> 8
			let mut res: (Ast<'a>, usize) = match self.kind() {
				Some(TokenKind::T_rparen) => { let la = self.shift()?.unwrap(); self.s16(AstNode::Leaf(la)) }
				Some(TokenKind::T_plus) => { let la = self.shift()?.unwrap(); self.s8(AstNode::Leaf(la)) }
				_ => Err(Error::from(self.err())),
			}?;
			loop {
				let (mut s, n) = res;
				if n == 0 {
					res = match *s.label() {
						_ => goto_err(),
					}?;
				} else {
					s.push(val);
					return Ok((s, n-1));
				};
			};
		}

		fn s13(&mut self, val: AstNode<NonTerm, Token<'a>>) -> Res<'a> {
			//13.
			//kernel:
			//	[Term -> int·, mult]*
			//	[Term -> int·, rparen]*
			//	[Term -> int·, plus]*
			//closure:
			//transitions:
			let mut res: (Ast<'a>, usize) = match self.kind() {
				Some(TokenKind::T_mult) => return ret(NonTerm::N_Term, val, 0),
				Some(TokenKind::T_rparen) => return ret(NonTerm::N_Term, val, 0),
				Some(TokenKind::T_plus) => return ret(NonTerm::N_Term, val, 0),
				_ => Err(Error::from(self.err())),
			}?;
			loop {
				let (mut s, n) = res;
				if n == 0 {
					res = match *s.label() {
						_ => goto_err(),
					}?;
				} else {
					s.push(val);
					return Ok((s, n-1));
				};
			};
		}

		fn s14(&mut self, val: AstNode<NonTerm, Token<'a>>) -> Res<'a> {
			//14.
			//kernel:
			//	[Fact -> Fact mult Term·, mult]*
			//	[Fact -> Fact mult Term·, plus]*
			//	[Fact -> Fact mult Term·, rparen]*
			//	[Fact -> Fact mult Term·, $]*
			//closure:
			//transitions:
			let mut res: (Ast<'a>, usize) = match self.kind() {
				Some(TokenKind::T_mult) => return ret(NonTerm::N_Fact, val, 2),
				Some(TokenKind::T_plus) => return ret(NonTerm::N_Fact, val, 2),
				Some(TokenKind::T_rparen) => return ret(NonTerm::N_Fact, val, 2),
				None => return ret(NonTerm::N_Fact, val, 2),
				_ => Err(Error::from(self.err())),
			}?;
			loop {
				let (mut s, n) = res;
				if n == 0 {
					res = match *s.label() {
						_ => goto_err(),
					}?;
				} else {
					s.push(val);
					return Ok((s, n-1));
				};
			};
		}

		fn s15(&mut self, val: AstNode<NonTerm, Token<'a>>) -> Res<'a> {
			//15.
			//kernel:
			//	[Sum -> Sum plus Fact·, plus]*
			//	[Sum -> Sum plus Fact·, $]*
			//	[Fact -> Fact ·mult Term, plus]*
			//	[Fact -> Fact ·mult Term, mult]*
			//	[Sum -> Sum plus Fact·, rparen]*
			//	[Fact -> Fact ·mult Term, rparen]*
			//	[Fact -> Fact ·mult Term, $]*
			//closure:
			//transitions:
			//	mult -> 7
			let mut res: (Ast<'a>, usize) = match self.kind() {
				Some(TokenKind::T_mult) => { let la = self.shift()?.unwrap(); self.s7(AstNode::Leaf(la)) }
				Some(TokenKind::T_plus) => return ret(NonTerm::N_Sum, val, 2),
				None => return ret(NonTerm::N_Sum, val, 2),
				Some(TokenKind::T_rparen) => return ret(NonTerm::N_Sum, val, 2),
				_ => Err(Error::from(self.err())),
			}?;
			loop {
				let (mut s, n) = res;
				if n == 0 {
					res = match *s.label() {
						_ => goto_err(),
					}?;
				} else {
					s.push(val);
					return Ok((s, n-1));
				};
			};
		}

		fn s16(&mut self, val: AstNode<NonTerm, Token<'a>>) -> Res<'a> {
			//16.
			//kernel:
			//	[Term -> lparen Sum rparen·, $]*
			//	[Term -> lparen Sum rparen·, plus]*
			//	[Term -> lparen Sum rparen·, rparen]*
			//	[Term -> lparen Sum rparen·, mult]*
			//closure:
			//transitions:
			let mut res: (Ast<'a>, usize) = match self.kind() {
				None => return ret(NonTerm::N_Term, val, 2),
				Some(TokenKind::T_plus) => return ret(NonTerm::N_Term, val, 2),
				Some(TokenKind::T_rparen) => return ret(NonTerm::N_Term, val, 2),
				Some(TokenKind::T_mult) => return ret(NonTerm::N_Term, val, 2),
				_ => Err(Error::from(self.err())),
			}?;
			loop {
				let (mut s, n) = res;
				if n == 0 {
					res = match *s.label() {
						_ => goto_err(),
					}?;
				} else {
					s.push(val);
					return Ok((s, n-1));
				};
			};
		}
	}

	impl<'a, L> Parser<'a, L>
	where
		L: Tokens<'a, TokenKind>,
	{
		pub fn new<I>(inp: I) -> Result<Self>
		where
			I: IntoIterator<IntoIter = L>,
		{
			let mut inp = inp.into_iter();
			let la = inp.next().transpose()?;
			Ok(Self {
				inp,
				la,
			})
		}

		pub fn parse(&mut self) -> Result<Ast<'a>> {
			let mut tree = self.s0()?;
			reverse(&mut tree);
			Ok(tree)
		}
	}

	impl<'a, L> ParserTrait<'a, L, TokenKind, NonTerm, Token<'a>> for Parser<'a, L>
	where
		L: Tokens<'a, TokenKind>,
	{
		fn new<I>(inp: I) -> Result<Self>
		where
			I: IntoIterator<IntoIter = L>,
		{
			Parser::new(inp)
		}

		fn parse(&mut self) -> Result<Ast<'a>> {
			Parser::parse(self)
		}
	}

	pub fn parse<'a, L, I>(iter: I) -> Result<Ast<'a>>
	where
		L: Tokens<'a, TokenKind>,
		I: IntoIterator<IntoIter = L>,
	{
		Parser::new(iter)?.parse()
	}

}
